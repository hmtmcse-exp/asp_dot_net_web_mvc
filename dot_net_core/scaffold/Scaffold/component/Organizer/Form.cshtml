@model MiaISoftMasterApp.Models.Organizer

<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control"/>
    <span class="has-error"><span class="help-block" asp-validation-for="Name" ></span></span>
</div>


<div class="form-group">
    <label asp-for="Email" class="control-label"></label>
    <input asp-for="Email" class="form-control"/>
    <span class="has-error"><span class="help-block" asp-validation-for="Email" ></span></span>
</div>


<div class="form-group">
    <label asp-for="Password" class="control-label"></label>
    <input asp-for="Password" class="form-control"/>
    <span class="has-error"><span class="help-block" asp-validation-for="Password" ></span></span>
</div>


<div class="form-group">
    <label asp-for="ConfirmPassword" class="control-label"></label>
    <input asp-for="ConfirmPassword" class="form-control"/>
    <span class="has-error"><span class="help-block" asp-validation-for="ConfirmPassword" ></span></span>
</div>


<div class="form-group">
    <label asp-for="OrganizerType" class="control-label"></label>
    <select asp-for="OrganizerType" asp-items="Html.GetEnumSelectList<MiaISoftMasterApp.Models.OrganizerType>()" class="form-control"></select>
    <span class="has-error"><span class="help-block" asp-validation-for="OrganizerType" ></span></span>
</div>

<div class="form-group">
    <label asp-for="UsageInterface" class="control-label"></label>
    <select asp-for="UsageInterface" asp-items="Html.GetEnumSelectList<MiaISoftMasterApp.Models.UsageInterface>()" class="form-control"></select>
    <span class="has-error"><span class="help-block" asp-validation-for="UsageInterface" ></span></span>
</div>


<div class="form-group">
    <div class="checkbox">
        <label class="control-label"><input asp-for="IsEnable"/>@Html.DisplayNameFor(model => model.IsEnable)</label>
    </div>
    <span class="has-error"><span class="help-block" asp-validation-for="IsEnable"></span></span>
</div>